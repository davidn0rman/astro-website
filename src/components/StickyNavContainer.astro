---
import MainNav from "./navtypes/MainNav.astro";
import MyspaceNav from "./navtypes/MyspaceNav.astro";

const { type } = Astro.props;

// Validate the 'type' prop
const allowedTypes = ["myspace", "main", "geocities"];
if (!allowedTypes.includes(type)) {
  throw new Error(`Invalid prop 'type': ${type}.`);
}
---

<>
  {type === "myspace" && <MyspaceNav />}
  {type === "main" && <MainNav />}
</>
